{
  "name": "big-math",
  "version": "1.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf \"+(test|src)/**/*+(.d.ts|.js|.map)\" .nyc_output coverage reports docs .stryker-tmp",
    "prebuild": "npm run clean && tslint -p tsconfig.json",
    "build": "tsc -p . && npm run docs",
    "mutate": "stryker run",
    "docs": "typedoc --out docs --exclude test/**/*.*",
    "test": "npm run build && nyc --include src/**/*.js --reporter=html --report-dir=coverage --check-coverage --lines 100 --functions 100 --branches 100 mocha \"test/**/*.js\" && npm run mutate"
  },
  "author": "kmdrGroch",
  "license": "MIT",
  "devDependencies": {
    "@stryker-mutator/core": "^1.0.1",
    "@stryker-mutator/html-reporter": "^1.0.1",
    "@stryker-mutator/mocha-framework": "^1.0.1",
    "@stryker-mutator/mocha-runner": "^1.0.1",
    "@stryker-mutator/typescript": "^1.0.1",
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.5",
    "chai": "^4.2.0",
    "mocha": "^5.2.0",
    "nyc": "^13.2.0",
    "rimraf": "^2.6.3",
    "tslint": "^5.12.1",
    "typescript": "^3.3.3"
  },
  "dependencies": {
    "typedoc": "^0.14.2"
  }
}
