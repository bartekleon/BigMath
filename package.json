{
  "name": "@kmdrgroch/big-math",
  "version": "1.3.0",
  "description": "Math library for big numbers",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf \"+(test|src|benchmark)/**/*+(.d.ts|.js|.map)\"",
    "full-clean": "npm run clean && rimraf .nyc_output coverage reports docs .stryker-tmp",
    "prebuild": "npm run full-clean && tslint -p tsconfig.json",
    "build": "tsc -p .",
    "mutate": "stryker run",
    "docs": "typedoc --out docs --exclude test/**/*.*",
    "test": "npm run build && nyc mocha \"test/**/*.js\" && npm run clean && npm run mutate",
    "start": "npm run test && tsc -p . && npm run docs"
  },
  "nyc": {
    "check-coverage": true,
    "lines": 100,
    "statements": 100,
    "functions": 100,
    "branches": 100,
    "include": [
      "src/**/*.js"
    ],
    "reporter": [
      "html"
    ],
    "plugins": [
      "bigInt"
    ],
    "report-dir": "./coverage"
  },
  "author": "kmdrGroch",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/plugin-syntax-bigint": "^7.2.0",
    "@stryker-mutator/core": "^1.1.1",
    "@stryker-mutator/html-reporter": "^1.1.1",
    "@stryker-mutator/mocha-framework": "^1.1.1",
    "@stryker-mutator/mocha-runner": "^1.1.1",
    "@stryker-mutator/typescript": "^1.1.1",
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.6",
    "chai": "^4.2.0",
    "mocha": "^6.0.2",
    "nyc": "^14.0.0",
    "rimraf": "^3.0.0",
    "ts-node": "^8.0.3",
    "tslint": "^5.14.0",
    "typedoc": "^0.14.2",
    "typescript": "^3.4.1"
  }
}
